#include <LiquidCrystal.h>



int SLT = 900;

int SST = 2000;

int STT = 600;

int END = 0;



LiquidCrystal lcd(7, 8, 9, 10, 11, 12);



void setup() {



lcd.begin(16, 0);



lcd.clear();

lcd.print(" BoxOS 1.5.0");

delay(SLT);

lcd.clear();



lcd.print("| BoxOS 1.5.0");

delay(SLT);

lcd.clear();



lcd.print("|_ BoxOS 1.5.0");

delay(SLT);

lcd.clear();



lcd.print("|_| BoxOS 1.5.0");

delay(SLT);

lcd.clear();



lcd.print("|_ BoxOS 1.5.0");

delay(SLT);

lcd.clear();



lcd.print("| BoxOS 1.5.0");

delay(SLT);

lcd.clear();



lcd.print(" BoxOS 1.5.0");

delay(SLT);

lcd.clear();



lcd.print("New major update!");

delay(SST);

lcd.clear();



lcd.print("What's new:");

delay(SST);

lcd.clear();



lcd.print("PW added.");

delay(SST);

lcd.clear();



lcd.print(".B.O.S.A added");

delay(SST);

lcd.clear();





lcd.print("Enter Password:");

delay(SST);

lcd.clear();



lcd.print("c");

delay(STT);

lcd.clear();



lcd.print(".o");

delay(STT);

lcd.clear();



lcd.print("..d");

delay(STT);

lcd.clear();



lcd.print("...e");

delay(STT);

lcd.clear();



lcd.print("....f");

delay(STT);

lcd.clear();



lcd.print(".....u");

delay(STT);

lcd.clear();



lcd.print("......n");

delay(STT);

lcd.clear();



lcd.begin(16, 2);



lcd.print("Login Sucsessful");

delay(SST);

lcd.clear();



lcd.print("BOSA loading");

delay(SLT);

lcd.clear();



lcd.print("BOSA loading.");

delay(SLT);

lcd.clear();



lcd.print("BOSA loading..");

delay(SLT);

lcd.clear();



lcd.print("BOSA loading...");

delay(SLT);

lcd.clear();



lcd.print("BOSA loading..");

delay(SLT);

lcd.clear();



lcd.print("BOSA loading.");

delay(SLT);

lcd.clear();



lcd.print("BOSA loading");

delay(1000);

lcd.clear();



lcd.print("o|");

delay(STT);

lcd.clear();



lcd.print("op|");

delay(STT);

lcd.clear();



lcd.print("ope|");

delay(STT);

lcd.clear();



lcd.print("open|");

delay(STT);

lcd.clear();



lcd.print("open |");

delay(STT);

lcd.clear();



lcd.print("open a|");

delay(STT);

lcd.clear();



lcd.print("open ap|");

delay(STT);

lcd.clear();



lcd.print("open app|");

delay(1035);

lcd.clear();



lcd.print("App:|");

delay(STT);

lcd.clear();



lcd.print("App:b|");

delay(STT);

lcd.clear();



lcd.print("App:br|");

delay(STT);

lcd.clear();



lcd.print("App:bro|");

delay(STT);

lcd.clear();



lcd.print("App:brow|");

delay(STT);

lcd.clear();



lcd.print("App:brows|");

delay(STT);

lcd.clear();



lcd.print("App:browse|");

delay(STT);

lcd.clear();



lcd.print("App:browser|");

delay(STT);

lcd.clear();



lcd.print(".");

delay(STT);

lcd.clear();



lcd.print("..");

delay(STT);

lcd.clear();



lcd.print("...");

delay(STT);

lcd.clear();



lcd.print("..");

delay(STT);

lcd.clear();



lcd.print(".");

delay(STT);

lcd.clear();



lcd.print("|");

delay(STT);

lcd.clear();



lcd.print("t|");

delay(STT);

lcd.clear();



lcd.print("te|");

delay(STT);

lcd.clear();



lcd.print("tem|");

delay(STT);

lcd.clear();



lcd.print("temp|");

delay(STT);

lcd.clear();



lcd.print("temp |");

delay(STT);

lcd.clear();



lcd.print("temp o|");

delay(STT);

lcd.clear();



lcd.print("temp ou|");

delay(STT);

lcd.clear();



lcd.print("temp out|");

delay(STT);

lcd.clear();



lcd.print("temp outs|");

delay(STT);

lcd.clear();



lcd.print("temp outsi|");

delay(STT);

lcd.clear();



lcd.print("temp outsid|");

delay(STT);

lcd.clear();



lcd.print("temp outside|");

delay(SST);

lcd.clear();



lcd.print("ERR_146_RDN_FAIL");

delay(SST);

lcd.clear();



lcd.print("Energy Saver");

delay(SST);

lcd.clear();



lcd.print("");

delay(END);

lcd.clear();

}

void loop() {

lcd.setCursor(0, 1);

}
